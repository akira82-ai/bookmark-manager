{
  "manifest_version": 3,
  "name": "Bookmark Manager Test",
  "version": "1.0",
  "description": "测试版本 - 书签管理插件",
  "permissions": [
    "storage",
    "tabs",
    "bookmarks",
    "history",
    "contextMenus",
    "notifications",
    "activeTab"
  ],
  "background": {
    "service_worker": "background-simple.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}