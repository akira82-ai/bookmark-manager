{
  "manifest_version": 3,
  "name": "书签管理与智能收藏提示",
  "version": "1.0",
  "description": "一款智能书签管理工具，可以帮助您整理书签、提供智能收藏建议，并提供用户友好的管理界面",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "bookmarks",
    "history",
    "storage",
    "tabs",
    "notifications",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; object-src 'self'"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "书签管理与智能收藏提示"
  },
  "options_page": "options.html"
}